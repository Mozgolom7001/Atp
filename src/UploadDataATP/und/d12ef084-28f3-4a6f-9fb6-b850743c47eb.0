			|	Командировка.Сотрудник = &Сотрудник
			|	И Командировка.ДатаНачала = &ДатаНачала"";
			Запрос.УстановитьПараметр(""Сотрудник"", Сотрудник);
			Запрос.УстановитьПараметр(""ДатаНачала"", ДанныеДокумента.ДатаНачала);
			
